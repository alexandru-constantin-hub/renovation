@model IEnumerable<Renovation.Models.Announce>

@{
    ViewData["Title"] = "Index";
}

<h1>Index</h1>

<p>
    <a asp-action="Create">Create New</a>
</p>
<table class="table">
    <thead>
        <tr>
            <th>
                @Html.DisplayNameFor(model => model.Adresse)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.Duree)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.Dimensione)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.AutreInformation)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.Etat)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.IdDesactivateurNavigation)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.IdPieceNavigation)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.IdTypeRenovationNavigation)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.IdUtilisateurNavigation)
            </th>
            <th></th>
        </tr>
    </thead>
    <tbody>
@foreach (var item in Model) {
        <tr>
            <td>
                @Html.DisplayFor(modelItem => item.Adresse)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.Duree)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.Dimensione)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.AutreInformation)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.Etat)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.IdDesactivateurNavigation.IdAdministrator)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.IdPieceNavigation.IdPiece)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.IdTypeRenovationNavigation.IdTypeRenovation)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.IdUtilisateurNavigation.IdUtilisateur)
            </td>
            <td>
                <a asp-action="Edit" asp-route-id="@item.IdAnnounce">Edit</a> |
                <a asp-action="Details" asp-route-id="@item.IdAnnounce">Details</a> |
                <a asp-action="Delete" asp-route-id="@item.IdAnnounce">Delete</a>
            </td>
        </tr>
}
    </tbody>
</table>
