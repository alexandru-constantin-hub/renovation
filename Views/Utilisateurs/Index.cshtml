@model IEnumerable<RenovationFinale.Models.Utilisateur>

@{
    ViewData["Title"] = "Index";
}

<h1>Index</h1>

<p>
    <a asp-action="Create">Create New</a>
</p>
<table class="table">
    <thead>
        <tr>
            <th>
                @Html.DisplayNameFor(model => model.Email)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.MotDePasse)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.Etat)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.Role)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.IdDesactivateur)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.IdActivateurNavigation)
            </th>
            <th></th>
        </tr>
    </thead>
    <tbody>
@foreach (var item in Model) {
        <tr>
            <td>
                @Html.DisplayFor(modelItem => item.Email)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.MotDePasse)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.Etat)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.Role)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.IdDesactivateur)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.IdActivateurNavigation.IdAdministrateur)
            </td>
            <td>
                <a asp-action="Edit" asp-route-id="@item.IdUtilisateur">Edit</a> |
                <a asp-action="Details" asp-route-id="@item.IdUtilisateur">Details</a> |
                <a asp-action="Delete" asp-route-id="@item.IdUtilisateur">Delete</a>
            </td>
        </tr>
}
    </tbody>
</table>
